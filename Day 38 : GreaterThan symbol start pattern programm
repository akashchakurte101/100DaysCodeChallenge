public class Main {
    public static void main(String[] args) {
        graterthanSymobolPattern(5);
    }
     public static void graterthanSymobolPattern(int n){
       for(int row=0; row<=2*n; row++){
           int totalColINRow= row>n ? n-(row-n+1) :row;
           for(int col=0 ; col<=totalColINRow; col++){
               System.out.print("*");
           }
           System.out.println();
       }
     }
}
