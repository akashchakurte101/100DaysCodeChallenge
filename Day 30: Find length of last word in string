public class LastWordLenght {
    public static void main(String[] args) {
        System.out.println(lastWordLenght("Press Shift twice to open the Search Everywhere"));
    }
    public static int lastWordLenght(String str) {
        String[] s=str.split(" ");
        int i=s.length-1;
        return s[i].length();
    }
}
