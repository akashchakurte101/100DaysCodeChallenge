class Main {
    public static void main(String[] args) {
       char c[]=new char[]{'(','{','}','('};
        System.out.println(validParentheses(c));
    }

    public static boolean validParentheses(char[] c) {
        int len=c.length;
        if (len % 2 != 0) {
            return false;
        }

        Stack<Character> st = new Stack<>();
        for (int i = 0; i < c.length; i++) {
            if (c[i] == '(' || c[i] == '[' || c[i] == '{') {
                st.push(c[i]);
            } else {
                if (st.empty()) {
                    return false;
                }
                char ch = st.pop();
                if ((c[i] == ')' && ch != '(') ||
                        (c[i] == ']' && ch != '[') ||
                        (c[i] == '}' && ch != '{')) {
                    return false;
                }
            }
        }
        return st.empty();
    }
}
